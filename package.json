{
  "name": "@kouts/vite-treeshakable-library",
  "description": "Vite treeshakable library",
  "version": "0.0.0-semantic-release",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.17.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host",
    "debug:hmr": "vite --host --debug hmr",
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.lib.json && resolve-tspaths --src ./lib --project tsconfig.lib.json && vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint-fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "test:unit": "vitest --project=unit",
    "test:unit-coverage": "vitest run --coverage --project=unit && make-coverage-badge",
    "typecheck": "vue-tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "vitest run --project=storybook",
    "quality": "pnpm run lint && pnpm run typecheck && vitest run --project=unit && vitest run --project=storybook && pnpm run build",
    "prepare": "husky"
  },
  "files": [
    "lib",
    "dist"
  ],
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./assets/lib.css": "./dist/vite-treeshakable-library.css"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "peerDependencies": {
    "vue": "^3.4.33"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@kouts/eslint-config": "^3.0.8",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "^9.1.8",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-links": "^9.1.8",
    "@storybook/addon-vitest": "^9.1.8",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/vue3-vite": "^9.1.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/compiler-core": "^3.5.22",
    "@vue/compiler-sfc": "^3.5.22",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-storybook": "^9.1.8",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.1",
    "make-coverage-badge": "^1.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "resolve-tspaths": "^0.8.23",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "storybook": "^9.1.8",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.8"
  }
}
