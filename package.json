{
  "name": "@kouts/vite-treeshakable-library",
  "description": "Vite treeshakable library",
  "version": "0.0.0-semantic-release",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.15.4",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host",
    "debug:hmr": "vite --host --debug hmr",
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.lib.json && resolve-tspaths --src ./lib --project tsconfig.lib.json && vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint-fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "test:unit": "vitest",
    "test:unit-coverage": "vitest run --coverage && make-coverage-badge",
    "typecheck": "vue-tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "prepare": "husky"
  },
  "files": [
    "lib",
    "dist"
  ],
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./assets/lib.css": "./dist/vite-treeshakable-library.css"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "peerDependencies": {
    "vue": "^3.4.33"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@kouts/eslint-config": "^2.0.4",
    "@playwright/test": "^1.49.1",
    "@storybook/addon-a11y": "^8.5.0",
    "@storybook/addon-actions": "^8.5.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/manager-api": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/theming": "^8.5.0",
    "@storybook/vue3": "^8.5.0",
    "@storybook/vue3-vite": "^8.5.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.1",
    "@vue/compiler-core": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-storybook": "^0.11.2",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.1",
    "make-coverage-badge": "^1.2.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "resolve-tspaths": "^0.8.23",
    "rimraf": "^6.0.1",
    "sass": "^1.83.4",
    "storybook": "^8.5.0",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0"
  }
}